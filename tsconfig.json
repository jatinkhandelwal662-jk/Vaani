{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "module": "ESNext",
    "moduleResolution": "bundler",

    "jsx": "react-jsx",

    "strict": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "noEmit": true,

    "useDefineForClassFields": false
  },
  "include": ["src", "App.tsx"]
}
